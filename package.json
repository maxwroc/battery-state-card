  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "rollup": "^2.34.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-minify-html-literals": "^1.2.5",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "adm-zip": "^0.5.0"
  },
  "dependencies": {}
}
